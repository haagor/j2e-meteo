<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                                  http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <navigation-rule>
        <from-view-id>/bulletinPres.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>PISTE</from-outcome>
            <to-view-id>/pistePres.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pistePres.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>BULLETIN</from-outcome>
            <to-view-id>/bulletinPres.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>